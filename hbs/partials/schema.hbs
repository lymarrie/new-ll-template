<script type="application/ld+json" class="yext-schema-json" data-yext-id="{{id}}">
{
    "@context":"https://schema.org",
    "@id":"{{id}}",
    "@type":["SelfStorage"],
    "address": {
        "@type":"PostalAddress",
        "addressLocality":"{{address.city}}",
        "addressRegion":"{{address.region}}",
        "postalCode":"{{address.postalCode}}",
        "streetAddress":"{{address.line1}}"
        },
    "description":"{{description}}",
    "name":"{{name}}",
    "openingHoursSpecification": {{>schemaHelpers-hours hours}},
    "image": "{{logo.image.url}}",
    "logo": "{{logo.image.url}}",
    "email":"reserve@localocker.com",
    "paymentAccepted":{{>schemaHelpers-payment paymentOptions}},
    "geo": {{>schemaHelpers-geo geocodedCoordinate}},
    "telephone":"{{phoneNumber 'US' mainPhone 'E164'}}",
    "url":"https://locations.localocker.com/{{slug}}",    
    "makesOffer": [
        {{>schemaHelpers-offers services}}
    ]
}
</script>
